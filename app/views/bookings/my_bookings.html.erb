<h1>My Bookings</h1>

<% @bookings.each do |booking|%>
  <li><h3><%= booking.island.name%></h3></li>
  <li><%= booking.check_in%></li>
  <li><%= booking.check_out%></li>
  <li><%= booking.guests%></li>
  <li><%= booking.island_id%></li>
  <li><%= booking.total_price%></li>

<% end %>
<%= link_to 'Back', islands_path %>